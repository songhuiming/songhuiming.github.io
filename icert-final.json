/*button listener for fiscal years in lcr and pjr*/
$("#h1b_data_series").not(".noCal").change(function(){
	var _fyVal = 	parseInt( $("#h1b_data_series").val() ) ;
	 /*sets color of date text*/
	 $("#start_date_from").css("color",  "#999");
	 $("#start_date_to").css("color",  "#999");
	 $("#end_date_from").css("color",  "#999");
	 $("#end_date_to").css("color",  "#999");
	 $("#create_date").val("mm/dd/yyyy").css("color",  "#000");
	 $("#post_end_date").val("mm/dd/yyyy").css("color",  "#000");

	/*Set datepicker calendar boundaries based on h1b_data_series (fiscal year) selected */
		var _fyDateLimits = { min: null,  max: null } ;
	/*The 2009 is an exception to the date range rule used for all years after 2009*/
	if( _fyVal == 2009 ) 
	{
		/*sets range for input fields*/
		_fyDateLimits.min = '04/15/2009' ;
		_fyDateLimits.max = '09/30/2009' ;
		/*decreased the date by one because the cal likes to increment it by 1*/
		var calLimitMin =parseInt( _fyVal - 1 ) + ', 09, 01, ';
		var calLimitMax =_fyVal + ', 08, 00, ';
		/*sets val input fields*/
		$( "#create_date" ).val( _fyDateLimits.min );
		$( "#post_end_date" ).val( _fyDateLimits.max );
		$( "#start_date_from" ).val( _fyDateLimits.min );
		$( "#start_date_to" ).val( _fyDateLimits.max );
		$( "#end_date_from" ).val( _fyDateLimits.min );
		$( "#end_date_to" ).val( _fyDateLimits.max );
		/*sets calendars*/
		minMaxdateLCR("init", "start_date_from", "2009, 03, 15", "2009, 09, 00", "start_date_to");
	    minMaxdateLCR("init", "end_date_from", "2009, 03, 15", "2009, 09, 00", "end_date_to");
	    minMaxdateLCR("init", "start_date_to", "2009, 03, 15", "2009, 09, 00", "start_date_from");
	    minMaxdateLCR("init", "end_date_to", "2009, 03, 15", "2009, 09, 00", "end_date_from");
		
	}
	else
	{
		/*for any year other than 2009 we need to set the FY range for calendars*/
		_fyDateLimits.min = '10/01/'+ parseInt( _fyVal - 1 ) ;
		_fyDateLimits.max = '09/30/'+ _fyVal  ;
		/*decreased the date by one because the cal likes to increment it by 1*/
		var calLimitMin =parseInt( _fyVal - 1 ) + ', 09, 01, '
		var calLimitMax =_fyVal + ', 09, 00, '
		/*sets val input fields*/
		$( "#create_date" ).val( _fyDateLimits.min );
		$( "#post_end_date" ).val( _fyDateLimits.max );
		$( "#start_date_from" ).val( _fyDateLimits.min );
		$( "#start_date_to" ).val( _fyDateLimits.max );
		$( "#end_date_from" ).val( _fyDateLimits.min );
		$( "#end_date_to" ).val( _fyDateLimits.max );
		/*sets calendars*/
		minMaxdateLCR("init", "start_date_from", calLimitMin, calLimitMax, "start_date_to");
		minMaxdateLCR("init", "start_date_to", calLimitMin, calLimitMax, "start_date_from");
	    minMaxdateLCR("init", "end_date_from", calLimitMin, calLimitMax, "end_date_to");
	    minMaxdateLCR("init", "end_date_to", calLimitMin, calLimitMax, "end_date_from");
	    
	}
});
