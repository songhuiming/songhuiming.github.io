/*
creates a calendar with a min and max date range
------------------------------------------------
selector - id of input
min - min date in (yyyy, mm, dd) format 
max - max date in (yyyy, mm, dd) format 
*/
function minMaxdate(selector, min, max){	
	/*creates arrays for min and max dates */
	var mindates = [];
  	var maxdates = [];

  	/*splits min dates and pushes them to arrays*/
	if(min !== undefined && min !== "today"){
  	min.split(/\s*, \s*/).forEach(function(dateType) {
	mindates.push(dateType);
	});}
	
	/*splits max dates and pushes them to arrays*/
	if(max !== undefined && max !== "today"){
  	max.split(/\s*, \s*/).forEach(function(dateType) {
  	maxdates.push(dateType);
	});}
	
	/*creates calendar with min and max dates */
	if(max !== undefined && min !== undefined){
	 $jq('#' + selector).pickadate({
	 min:  new Date(mindates[0], mindates[1], mindates[2]), 
	 max:  new Date(maxdates[0], maxdates[1], maxdates[2]), 
	 today:  false, 
	 clear:  false, 
     close:  false, 
	 format:  'mm/dd/yyyy'
	});	
	}
	/*creates calendar with just min date */
	else if(min !== undefined && max == undefined){
	 $jq('#' + selector).pickadate({
	 min:  new Date(mindates[0], mindates[1], mindates[2]), 
	 today:  false, 
	 clear:  false, 
     close:  false, 
	 format:  'mm/dd/yyyy'
	});	
	}
	/*creates calendar with just max date*/
	else if(max !== undefined && min == undefined){
	 $jq('#' + selector).pickadate({
	 max:  new Date(maxdates[0], maxdates[1], maxdates[2]), 
	 today:  false, 
	 clear:  false, 
     close:  false, 
	 format:  'mm/dd/yyyy'
	});}	

	/*creates calendar with min or max date of today ==*/
	else if(min == "today" || max == "today"){
	if(min == "today"){
	$jq('#' + selector).pickadate({
	 min:  new Date(), 
	 format:  'mm/dd/yyyy'
	});
	}else if(max == "today"){
	$jq('#' + selector).pickadate({
	 max:  new Date(), 
	 format:  'mm/dd/yyyy'
	});}}

}



