<script>
	function setFieldFocus(elt) {
		var targetField = "";
		if ($("#popupBox .firstFocusField input[type!='hidden']",  elt.content).length > 0)
			var targetField = $("#popupBox .firstFocusField input[type!='hidden']",  elt.content);
		else if ($("#popupBox input: text: first",  elt.content).length > 0)
			var targetField = $("#popupBox input: text: first",  elt.content);
		else if ($("#popupBox textarea: first",  elt.content).length > 0)
			var targetField = $("#popupBox textarea: first",  elt.content);
		else if ($("#popupBox input: button: first",  elt.content).length > 0)
		var targetField = $("#popupBox input: button: first",  elt.content);
		else if ($("#popupBox select: first",  elt.content).length > 0)
			var targetField = $("#popupBox select: first",  elt.content);
		else if ($("#popupBox .firstFocusField[hidden]",  elt.content).length > 0)
			var targetField = $("#popupBox select: first",  elt.content);
		else if ($("#popupBox .buttonHolder input[type!='hidden']: first",  elt.content).length > 0)
			var targetField = $("#popupBox .buttonHolder input[type!='hidden']: first",  elt.content);
		if (targetField.length > 0) {
			targetField.focus(); 
		}
	}

	$(document).ready(function(){
		
		
		$('#editData1,  #editData2,  #editData3,  #editData4,  #editData5,  #editData6,  #editData7').removeAttr('target').bind("submit",  function(event){
			event.preventDefault();
			$(this).nyroModalManual({modal:  true, minWidth:  550, minHeight:  400,  endShowContent:  function(elt,  settings) {setFieldFocus(elt);}});
			return false;
		});
		
		$('#editData,  #editData9').removeAttr('target').bind("submit",  function(event){
			event.preventDefault();
			$(this).nyroModalManual({modal:  true, minWidth:  700, minHeight:  400,  endShowContent:  function(elt,  settings) {setFieldFocus(elt);}});
			return false;
		}); 
		
		$('#editData8, #editData10').removeAttr('target').bind("submit",  function(event){
			event.preventDefault();
			$(this).nyroModalManual({modal:  true, minWidth:  700, minHeight:  600,  endShowContent:  function(elt,  settings) {setFieldFocus(elt);}});
			return false;
		}); 
	
		$('#editData9999').removeAttr('target').bind("submit",  function(event){
			event.preventDefault();
			$(this).nyroModalManual();			
			return false;
		});	
	});

function doPDFAddendum(rowid){
				$.nyroModalManual(
							{
							url:  'index.cfm?event=ehH2AQueue.pdf', 
							ajax:  {data:  'caseId='+rowid,  type:  'POST'}, 
							type:  'form', 
							modal:  true, 
							bgColor:  '#666666', 
							width:  550, 
							height:  200, 
							minWidth:  500,  // Minimum width
							minHeight:  200,  // Minimum height
							//resizeable:  true,  // Indicate if the content is resizable. Will be set to false for swf
							autoSizable:  true, 
							endShowContent:  function(elt,  settings) {setFieldFocus(elt);}});
			}

