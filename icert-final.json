		if(actionType == "add"){
	  	getInactivationDate();
	  	}	

	  	if(actionType == "h2aStep7"){
	  	dateCompare("startofWorkDate",  "endofWorkDate",  "h2aStep7");
	  	}

	  }});
	}
	  /*creates calendar with onStart action (when calendar object is created)*/
	if(action =="onStart"){
	 
	 if(selectorType == "class"){
	  	var selecttorr = "."
	  }else{
	  	var selecttorr = "#"}

	 $jq(selecttorr + date1).pickadate({
	  labelMonthNext:  'Go to the next month', 
	  labelMonthPrev:  'Go to the previous month', 
	  labelMonthSelect:  'Pick a month from the dropdown', 
	  labelYearSelect:  'Pick a year from the dropdown', 
	  format:  'mm/dd/yyyy', 
	  today:  false, 
	  clear:  false, 
	  close:  false, 
	  editable: false, 
	  onStart:  function() {

	 }
	 });}  

	 /*creates calendar with onSelect action*/
	 if(action =="onSelect"){

	  	if(selectorType == "class"){
	  	var selecttorr = "."
	  }else{
	  	var selecttorr = "#"}

	 $jq(selecttorr + date1).pickadate({
	  labelMonthNext:  'Go to the next month', 
	  labelMonthPrev:  'Go to the previous month', 
	  labelMonthSelect:  'Pick a month from the dropdown', 
	  labelYearSelect:  'Pick a year from the dropdown', 
	  format:  'mm/dd/yyyy', 
	  today:  false, 
	  clear:  false, 
	  close:  false, 
