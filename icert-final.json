	if($(this).hasClass("508Focus")){
	/*adds blue border around focus items*/
 	$(this).css({border: "2px solid #1055A5"});
 	} else{
 	/*grabs previous color*/
 	origColor = $(this).css("color");
 	/*changes text color to blue*/
 	$(this).css({color: 'blue'});}
 	
 	/*blur listener*/
    }).blur(function() {
    /*checks if class is focus or focusNoBorder*/	
    if($(this).hasClass("508Focus")){	 
    /*removes border*/
 	$(this).css({border: ""});
 	} else{
 	/*removes blue color*/
 	$(this).css({color: origColor});
