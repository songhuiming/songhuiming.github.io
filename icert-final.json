	  	}

	 }
	 });}

}

/*
creates calendar that exceutes action 
------------------------------------------------
date1 - id of input
selectorType - id or class 
actionType - the type of action you want to excute
*/
function lcrDateCompare(date1){
	$jq('#' + date1).pickadate({
	 today:  false, 
	 clear:  false, 
     close:  false, 
     closeOnSelect:  false, 
	 format:  'mm/dd/yyyy', 
	 onClose:  function() {
	 /*comapares date to earliest start date then to its corresponding date field if defined*/
	var startDt = $("#" + date1).val();
	var endDt = '04/15/2009';
	if( new Date(startDt).getTime() < new Date(endDt).getTime()){
		alert('Only certifications issued on or after April 15,  2009 are available in LCJR.  Please select a date on or after April 15,  2009.');
		$("#start_date_from").val('04/15/2009')
	}
	if(date2 !== undefined){
		if(date1 == "#start_date_from" || date1 == "end_date_from"){
			/*passes fields to comapare dates */
		dateCompare(date1, date2, "lcr")
		}else{
			/*passes fields to comapare dates */
			dateCompare(date2, date1, "lcr")
		}}
	}
	});
}

/*
