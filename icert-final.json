/*
compares two dates 
------------------------------------------------
dateInputFrom - id of input
dateInputTo - id of date want to compare 
alertType - the type of the var of the alert you want to display 
*/
function dateCompare(dateInputFrom, dateInputTo, alertType){

	/*sets dates to a var*/
	var date1 = $("#" + dateInputFrom).val();
	var date2 = $("#" + dateInputTo).val();
	/*sets date format*/
	var d1 = new Date(date1);
	var d2 = new Date(date2);
	/*checks if dates or equal or greater theb each other*/
	var same = d1.getTime() === d2.getTime();
	var greater = d1.getTime() > d2.getTime();
	var toLessThan = d2.getTime() < d1.getTime();

	/*if dates are equal*/
	if(same == true){
	if( alertType == "lcr"){
	alert('Please enter the Work Start To Date greater than Work Start From Date');
	$("#" + dateInputFrom).val("");
	$("#" + dateInputTo).val("");
	}
	if( alertType == "h2aStep7"){
	alert('Please enter the Work Start To Date greater than Work Start From Date');
	$("#" + dateInputFrom).val("");
	$("#" + dateInputTo).val("");
	}
	/*if date from is greater then date to */
	}else if(greater == true){
	if( alertType == "lcr"){
	alert('Please enter the Work Start To Date greater than Work Start From Date');
	$("#" + dateInputFrom).val("");
	$("#" + dateInputTo).val("");	
	/*if to date is less then from date*/
	}else if(toLessThan == true){
	alert('Please enter "To" date that is greater than "To"');
	$("#" + dateInputFrom).val("");
	$("#" + dateInputTo).val("");
	}else{
	return false;
	}

}

}

